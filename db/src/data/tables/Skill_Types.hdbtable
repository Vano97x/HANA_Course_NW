COLUMN TABLE "SKILL_TYPES"(
    "SKILL_TYPE_ID" INTEGER unique NOT NULL  COMMENT 'Skill Type ID',
    "SKILL_TYPE_NAME" CHAR(20) NOT NULL COMMENT 'Skill Type Name',
    "DESCRIPTION" CHAR(100) COMMENT 'Skill Type Description',
    PRIMARY KEY ("SKILL_TYPE_ID")
) WITH ASSOCIATIONS(JOIN "SKILLS" AS "SKL" ON "SKILL_TYPE_ID" = "SKL"."SKILL_TYPE_ID" ) 